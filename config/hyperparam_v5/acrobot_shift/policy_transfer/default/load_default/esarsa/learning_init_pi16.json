{
  "comment": "This is the config file for expected SARSA lambda agent for the continuing cartpole environment.",
  "agent-name": "esarsa",
  "environment-name": "acrobot",
  "agent-settings": {
    "gamma": 1.0,
    "state-len": 6,
    "env-name": "acrobot",
    "sweep": {
      "tilings": [16],
      "tiles": [8],
      "is-stepsize-adaptive": [true],
      "alpha": [0.0],
      "lambda": [0.8],
      "epsilon": [0.0],
      "adaptive-alpha": [0.003, 0.03, 0.3],
      "beta1": [0.0, 0.9],
      "softmax-temp": [1.0, 10.0, 100.0],
      "weight-init": [0.0, 8.0, 16.0]
    },

    "weight-load": true,
    "weight-load-path": "data/hyperparam_v5/acrobot_shift/online_learning/default/esarsa/step15k/sweep/param_{}/weight-16.pkl",

    "lock-weight": false,

    "enable-debug": false,
    "seed": 1
  },
  "environment-settings": {
    "seed": 1
  },
  "experiment-settings": {
    "randomize_start_state_beforeLock": false,
    "randomize_start_state_afterLock": false,
    "steps": 0,
    "episodes": 1000000,
    "max-run-length-episodic": 15000,
    "data-path": "data/hyperparam_v5/acrobot_shift/policy_transfer/default/load_default/esarsa/learning_init_pi16/",

    "should-log-totals": true,
    "should-log-episode-lengths": true,
    "debug-interval": 0
  }
}