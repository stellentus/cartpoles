{
  "agent-name": "dqn",
  "environment-name": "cartpole",
  "agent-settings": {
    "epsilon": 0.2,
    "decreasing-epsilon": "None",
    "gamma": 0.9,
    "state-len": 4,
    "buffer-type": "random",
    "optimizer": "Adam",
    "dqn-adamBeta1": 0.9,
    "dqn-adamBeta2": 0.999,
    "dqn-adamEps": 1e-8,
    "sweep": {
      "alpha": [0.001],
      "dqn-hidden": [
        [128, 128]
      ],
      "dqn-sync":[32],
      "dqn-batch":[64],
      "buffer-size":[100]
    },

    "lock-weight": true,
    "lock-condition": "beginning",

    "enable-debug": false,
    "seed": 100
  },
  "environment-settings": {
    "seed": 100,
    "sweep": {
      "delays":[[0]]
    }
  },
  "experiment-settings": {
    "steps": 0,
    "episodes": 10000000,
    "total-step-after-lock": 10000,
    "count-step-after-lock": true,

    "data-path": "data/hyperparam/cartpole/offline_data/dqn/step10k_env/lockat_random/",
    "should-log-traces": true,
    "should-log-episode-lengths": false,
    "debug-interval": 1000
  }
}
