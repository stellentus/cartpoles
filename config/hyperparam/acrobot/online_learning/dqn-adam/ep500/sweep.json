{
  "agent-name": "dqn",
  "environment-name": "acrobot",
  "agent-settings": {
    "decreasing-epsilon": "None",
    "gamma": 1.0,
    "state-len": 6,
    "buffer-type": "random",
    "optimizer": "Adam",
    "dqn-adamBeta1": 0.9,
    "dqn-adamBeta2": 0.999,
    "dqn-adamEps": 1e-8,
    "sweep": {
      "alpha": [1e-3, 1e-4],
      "dqn-hidden": [
        [128, 128], [256, 256]
      ],
      "dqn-sync":[32],
      "dqn-batch":[64],
      "buffer-size":[2500],
      "epsilon": [0.1, 0.05, 0]
    },
    "lock-weight": false,
    "enable-debug": false,
    "seed": 1
  },
  "environment-settings": {
    "seed": 1
  },
  "experiment-settings": {
    "steps": 0,
    "episodes": 500,
    "data-path": "data/hyperparam/acrobot/online_learning/dqn-adam/ep500/sweep",
    "should-log-traces": false,
    "should-log-episode-lengths": true,
    "debug-interval": 1000
  }
}
