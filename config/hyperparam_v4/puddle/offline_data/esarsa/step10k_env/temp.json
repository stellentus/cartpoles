{
  "agent-name": "esarsa",
  "environment-name": "puddleworld",
  "agent-settings": {
    "env-name": "puddleworld",
    "gamma": 1,
    "state-len": 2,
    "buffer-type": "random",
    "buffer-size": 1,
    "decreasing-epsilon": "None",
    "sweep": {
      "weight-init": [1],
      "epsilon": [1],
      "epsilon-after-lock": [1],
      "tilings": [4],
      "tiles": [4],
      "lambda": [0.9],
      "alpha": [0.2],
      "is-stepsize-adaptive": [false],
      "adaptive-alpha": [0]
    },

    "lock-weight": true,
    "lock-condition": "beginning",

    "enable-debug": false,
    "seed": 100
  },
  "environment-settings": {
    "seed": 100
  },
  "experiment-settings": {
    "randomize_start_state_beforeLock": false,
    "randomize_start_state_afterLock": false,
    "steps": 0,
    "episodes": 100000,
    "total-step-after-lock": 10000,
    "count-step-after-lock": true,
    "data-path": "data/hyperparam_v4/puddle/offline_data/esarsa/step10k_env/temp/",
    "should-log-traces": true,
    "should-log-episode-lengths": false,
    "debug-interval": 1000
  }
}
