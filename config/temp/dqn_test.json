{
  "agent-name": "dqn",
  "environment-name": "cartpole",
  "agent-settings": {
    "epsilon": 0.2,
    "decreasing-epsilon": "None",
    "gamma": 0.9,
    "state-len": 4,
    "buffer-type": "random",
    "dqn-adamBeta1": 0.9,
    "dqn-adamBeta2": 0.999,
    "dqn-adamEps": 1e-8,
    "optimizer": "Adam",
    "sweep": {
      "alpha": [0.001],
      "dqn-hidden": [
        [64, 64]
      ],
      "dqn-sync":[32],
      "dqn-batch":[64],
      "buffer-size":[100]
    },

    "lock-weight": false,
    "lock-condition": "onetime-reward",
    "lock-condition-reward": -0.005,

    "enable-debug": false,
    "seed": 100
  },
  "environment-settings": {
    "seed": 100,
    "sweep": {
      "delays":[[0]]
    }
  },
  "experiment-settings": {
    "randomize_start_state_beforeLock": false,
    "randomize_start_state_afterLock": false,
    "steps": 50000,
    "data-path": "data/temp/cartpole/offline_data/dqn/step10k_env/lockat_baseline/",
    "should-log-traces": false,
    "should-log-episode-lengths": false,
    "should-log-rewards": false,
    "should-log-totals": true,
    "debug-interval": 1000
  }
}