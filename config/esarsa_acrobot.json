{
	"comment": "This is the config file for expected SARSA lambda agent for the continuing cartpole environment.",
	"agent-name": "esarsa_acrobot",
	"environment-name": "acrobot",
	"agent-settings": {
		"tilings": 32,
		"tiles": 4,
		"gamma": 1.0,
		"lambda": 0.8,
		"epsilon": 0.05,
		"alpha": 0.1,
		"adaptive-stepsize": true,
		"adaptive-alpha": 0.001,
		"sweep": {
			"tilings": [64],
			"tiles": [8],
			"gamma": [1.0],
			"lambda": [0.7],
			"epsilon": [0.05],
			"alpha": [0.01],
			"is-stepsize-adaptive": [true],
			"adaptive-alpha": [1e-3]
		},
		"enable-debug": false,
		"seed": 1
	},
	"environment-settings": {
		"seed": 1,
		"sweep":{
			"delays": [[0]]
		}
	},
	"experiment-settings": {
        "episodes": 10,
        "steps-in-episode": 500,
		"data-path": "data/esarsa_acrobot",
		"should-log-traces": true,
		"should-log-episode-lengths": true,
		"debug-interval": 500
	}
